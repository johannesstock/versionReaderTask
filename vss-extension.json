{
  "manifestVersion": 1,
  "id": "VersionReaderTaskExtended",
  "name": "Version Reader Extended",
  "version": "1.0",
  "publisher": "johannesstock",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A build task that reads the <Version> tag from the .csproj or .vbproj file and makes it available as build variables. Also Includes The single Version Groups",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "Project",
    "Version",
    "Build Variables"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "VersionReaderTask"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/johannesstock/versionReaderTask"
    },
    "support": {
      "uri": "https://github.com/johannesstock/versionReaderTask"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/johannesstock/versionReaderTask"
  },
  "galleryFlags": [
    "Public"
  ],
  "contributions": [
    {
      "id": "version-reader-build-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "VersionReaderTask"
      }
    }
  ]
}
